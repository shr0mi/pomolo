<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="rootPane" prefHeight="540.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pages.all_songs.AllSongsPageController">
    <children>
        <Text fill="WHITE" layoutX="300.0" layoutY="52.0" strokeType="OUTSIDE" strokeWidth="0.0" text="ADD SONGS FROM LIBRARY" textAlignment="CENTER">
            <font>
                <Font name="Monospaced Regular" size="25.0" />
            </font>
        </Text>
        <ScrollPane fitToWidth="true" hbarPolicy="NEVER" layoutX="35.0" layoutY="88.0" prefHeight="380.0" prefWidth="906.0">
            <content>
                <VBox fx:id="vbox" prefWidth="892.0">
                    <padding>
                        <Insets bottom="60.0" />
                    </padding>
                </VBox>
            </content>
        </ScrollPane>
        <HBox alignment="CENTER_LEFT" layoutX="35.0" layoutY="30.0" prefWidth="906.0" spacing="10.0">
            <children>
                <Button onAction="#addSelectedSongs" text="Add Selected to Playlist" styleClass="close-btn, add-playlist-btn" />
                <HBox HBox.hgrow="ALWAYS" />
                <Button onAction="#goBack" text="Back" styleClass="close-btn" />
            </children>
        </HBox>
    </children>
</AnchorPane>
